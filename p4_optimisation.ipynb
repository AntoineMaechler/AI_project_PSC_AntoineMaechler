   "source": [
    "# Fonction d'utilité\n",
    "def utility_function(m):\n",
    "    pexp = np.exp(beta*(p+m))\n",
    "    pit = pexp/np.sum(pexp)\n",
    "    return(-np.sum(m*pit/p))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fonction de contrainte\n",
    "def constraint_function(m):\n",
    "    pexp = np.exp(beta*(p+m))\n",
    "    pit = pexp/np.sum(pexp)\n",
    "    return(np.sum(pit*CO2reel))"
   ]
